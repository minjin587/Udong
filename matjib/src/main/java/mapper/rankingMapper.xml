<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapper.Rankingmapper">
	<select id="listStore" resultType="model.Store">
	select store_no , store_name, store_addr , store_context
	from store_table order by store_no	
	</select>
	
</mapper>
